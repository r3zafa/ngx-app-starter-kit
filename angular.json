{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-app-starter-kit": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "configs/polyfills.ts"
            ],
            "tsConfig": "./configs/tsconfig.app.json",
            "allowedCommonJsDependencies": [
              "maplibre-gl"
            ],
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public/fonts",
                "output": "/fonts/"
              },
              {
                "glob": "**/*",
                "input": "public/i18n",
                "output": "/i18n/"
              },
              {
                "glob": "**/*",
                "input": "public/icons",
                "output": "/icons/"
              },
              {
                "glob": "favicon.ico",
                "input": "public",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "serviceWorker": "ngsw-config.json",
              "assets": [
                {
                  "glob": "**/*",
                  "input": "public/fonts",
                  "output": "/fonts/"
                },
                {
                  "glob": "**/*",
                  "input": "public/i18n",
                  "output": "/i18n/"
                },
                {
                  "glob": "**/*",
                  "input": "public/icons",
                  "output": "/icons/"
                },
                {
                  "glob": "favicon.ico",
                  "input": "public",
                  "output": "/"
                },
                "src/manifest.webmanifest"
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4MB",
                  "maximumError": "8MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "512kB",
                  "maximumError": "1MB"
                }
              ],
              "outputHashing": "all",
              "sourceMap": {
                "scripts": true,
                "styles": false,
                "hidden": true,
                "vendor": true
              },
              "namedChunks": false,
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "fonts": true
              },
              "extractLicenses": true
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            },
            "staging": {
              "sourceMap": true,
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "fonts": true
              },
              "outputHashing": "all",
              "extractLicenses": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngx-app-starter-kit:build:production"
            },
            "development": {
              "buildTarget": "ngx-app-starter-kit:build:development"
            },
            "staging": {
              "buildTarget": "ngx-app-starter-kit:build:staging"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "configs/polyfills.test.ts"
            ],
            "karmaConfig": "configs/karma.conf.js",
            "tsConfig": "configs/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}